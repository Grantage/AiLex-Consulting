        }
      }
    ]
  }
  </script>
  
  <script>
    document.addEventListener('DOMContentLoaded', function() {
      const purposeSelect = document.getElementById('ai-purpose-select');
      const checkButton = document.getElementById('check-risk-button');
      const resultDiv = document.getElementById('risk-result');
      const highRiskResult = document.getElementById('high-risk-result');
      const gpaiResult = document.getElementById('gpai-result');
      const lowRiskResult = document.getElementById('low-risk-result');

      if (checkButton && purposeSelect) {
        checkButton.addEventListener('click', function() {
          const purpose = purposeSelect.value;
          
          if (!purpose) {
            alert('Please select your AI system\'s primary purpose');
            return;
          }

          // Hide all results first
          highRiskResult.classList.add('hidden');
          gpaiResult.classList.add('hidden');
          lowRiskResult.classList.add('hidden');

          // Show appropriate result based on selection
          if (['biometric', 'infrastructure', 'education', 'employment', 'essential-services', 'law-enforcement', 'migration', 'justice', 'safety-component'].includes(purpose)) {
            highRiskResult.classList.remove('hidden');
          } else if (purpose === 'general-purpose') {
            gpaiResult.classList.remove('hidden');
          } else {
            lowRiskResult.classList.remove('hidden');
          }

          resultDiv.classList.remove('hidden');

          // Track interaction
          if (typeof window.trackConversion !== 'undefined') {
            window.trackConversion('ai_risk_classification_used', {
              purpose: purpose,
              timestamp: new Date().toISOString()
            });
          }

          // Scroll to result
          resultDiv.scrollIntoView({ behavior: 'smooth', block: 'start' });
        });
      }
    });
